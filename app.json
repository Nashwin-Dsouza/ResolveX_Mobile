{
  "expo": {
    "name": "ResolveX",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_v2.png",
    "scheme": "resolvex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nashwin.resolvex"
    },

    "android": {
      "icon": "./assets/images/icon_v2.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon_v2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nashwin.resolvex",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ]
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos so you can upload proof.",
          "cameraPermission": "The app needs access to your camera so you can take a photo for proof."
        }
      ],
      "expo-secure-store"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "dde1b53b-dbb7-4189-896e-8039b5b7e6ed"
      }
    },

    "owner": "nashwin_dsouza"
  }
}
